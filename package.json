{
  "name": "samoc-server",
  "version": "5.12.0",
  "description": "SAMOC API Server",
  "main": "dist/start.js",
  "author": "",
  "license": "MIT",
  "scripts": {
    "clean": "./scripts/clean.sh",
    "download-logs:aws-dev": "./scripts/download-logs-aws.sh dev",
    "download-logs:aws-prod": "./scripts/download-logs-aws.sh prod",
    "lint": "tsc && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve": "nodemon dist/start.js",
    "start": "node --inspect=5858 -r ts-node/register ./src/start.ts",
    "start:watch": "nodemon",
    "build": "yarn run tsc",
    "test": "yarn jest --forceExit --coverage --detectOpenHandles --runInBand --silent",
    "test:int": "yarn test test/integrations/*test*",
    "sqldump:aws-dev": "timestamp=$(date +\"%Y_%m_%d_%H_%M_%S\") && mysqldump --column-statistics=0 -h samoc-dev.cluster-cj2xymapkrpz.us-west-2.rds.amazonaws.com -u svc_samoc-dev -p 4ZObnUguD6 samoc-dev > db/samoc_dev_aws-$timestamp-dump.sql",
    "postinstall": "husky install"
  },
